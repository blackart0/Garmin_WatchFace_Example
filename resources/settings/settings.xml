<settings>

	<setting propertyKey="@Properties.appVersion" title="@Strings.AppVersionTitle">
        <settingConfig type="alphaNumeric" readonly="true" /> 
    </setting>
    
    <setting propertyKey="@Properties.weather-provider" title="@Strings.weatherProviderTitle">
        <settingConfig type="list">
            <listEntry value="0">@Strings.openWeatherMapTitle</listEntry>
            <listEntry value="1">@Strings.garminWeatherTitle</listEntry>
            <listEntry value="2">@Strings.qWeatherTitle</listEntry>
        </settingConfig>   
    </setting>
    
